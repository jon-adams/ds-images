{
    "extends": "tslint:recommended",
    "rules": {
        "max-classes-per-file": 0,
        "max-line-length": {
            "options": [120]
        },
        "new-parens": true,
        "no-arg": true,
        "no-consecutive-blank-lines": true,
        "no-console": false
    },
    "jsRules": {
        "max-line-length": {
            "options": [120]
        }
    }
}
