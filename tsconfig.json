{
    "compilerOptions": {
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noUnusedLocals": true,
        "removeComments": true,
        "sourceMap": true,
        "lib": [
            "dom",
            "es5",
            "es2015.promise",
            "es2015.iterable",
            "esnext.asynciterable"
        ],
        "outDir": "./dist/"
    },
    "include": ["handler.ts", "src/*.ts", "test/*.ts"],
    "exclude": [".webpack", "node_modules"]
}
